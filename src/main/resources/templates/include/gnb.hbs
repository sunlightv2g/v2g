<div class="top">
    <h1><a href="/">VEHECLE To GRID SYSTEM</a></h1>
    <ul>
    	{{#session}}
    	<li><a href="#" onclick="logout(); return false;">로그아웃</a></li>
    	{{else}} 
    	<li><input type="text" id="loginid" maxlength="50" placeholder="ID"></li>
	    <li><input type="password" id="loginpass" maxlength="50" placeholder="PW"></li>
	    <li><a href="#" onclick="userlogin();return false;">로그인</a></li>
	    
	    {{/session}}
	    
    </ul>
</div><!-- top end-->
<div class="options">
    <ul class="inputs">
        <li>
            <select name="sido" id="sido" onchange="categoryChange(this)">
                <option value="">시/도</option>
                <option value="서울">서울</option>
                <option value="경기">경기</option>
                <option value="강원">강원</option>
                <option value="충청북도">충청북도</option>
                <option value="충청남도">충청남도</option>
                <option value="전라북도">전라북도</option>
                <option value="전라남도">전라남도</option>
                <option value="경상북도">경상북도</option>                
                <option value="경상남도">경상북도</option>                
                <option value="제주도">경상북도</option>                                
            </select>
        </li>
        <li>
            <select name="gumyun" id="gumyun">
                <option value="">구/읍/면</option>

            </select>
        </li>
        <li><input type="text" name="searchStr" id="searchStr" placeholder="공동주택명" class="black"></li>
        <li><button type="button" onclick="seachSubmit(); return false;">선택</button></li>
    </ul>
    <ul class="buttons">
        <li><a href="/customer" {{#eq memudata "menu1"}}style="background: url(../img/btn_bg_on.png) no-repeat;"{{/eq}}>고객기준</a></li>
        <li><a href="/trade" {{#eq memudata "menu2"}}style="background: url(../img/btn_bg_on.png) no-repeat;"{{/eq}}>전력거래</a></li>
        <li><a href="/analysis" {{#eq memudata "menu3"}}style="background: url(../img/btn_bg_on.png) no-repeat;"{{/eq}}>V2G분석</a></li>
        <li><a href="/reservation" {{#eq memudata "menu4"}}style="background: url(../img/btn_bg_on.png) no-repeat;"{{/eq}}>예약화면</a></li>
        <li><a href="/environment" {{#eq memudata "menu5"}}style="background: url(../img/btn_bg_on.png) no-repeat;"{{/eq}}>환경설정</a></li>
    </ul>
</div><!-- options end-->
<div class="status">
<h2>전체현황</h2>
<p>현재시간<span class="time">{{{weatherWrite}}}</span></p>
<ul>
    <li>
        <span class="num_five">{{formatStringNumber centerdata.carcnt}}</span>대
        <span class="skyblue">차량 Total</span>
    </li>
    <li>
        <span class="num_five">{{formatStringNumber centerdata.vcar}}</span>대
        <span class="skyblue">금일V2G차량 Total</span>
    </li>
    <li>
        <span class="num_six">{{formatStringNumber centerdata.vcost}}</span>천원
        <span class="skyblue">금일 V2G 금액 Total</span>
    </li>
    <li>
        <span class="num_five">{{formatStringNumber centerdata.velectric}}</span>MWh
        <span class="skyblue">금일 V2G 전력량 Total </span>
    </li>
    <li>
        <span class="num_six">{{formatStringNumber centerdata.vaccure}}</span>MWh
        <span class="skyblue">누적 V2G 전력량 Total</span>
    </li>
</ul>
</div><!-- status end-->